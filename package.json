{
  "name": "studip-opencast-plugin",
  "version": "3.0.14",
  "description": "Stud.IP plugin for Opencast",
  "author": "Till Gl√∂ggler <tgloeggl@uos.de>",
  "scripts": {
    "build": "composer install --no-dev && npm install && webpack --mode production -d source-map && cd courseware; sh prebuild.sh",
    "devcw": "cd courseware; sh watch.sh",
    "predev": "npm install",
    "dev": "webpack --mode development --watch",
    "prezip": "npm run build",
    "translate": "gettext-extract --attribute v-i18n --attribute v-translate --output locale/en/LC_MESSAGES/opencast_js.pot $(find vueapp -type f -name '*.vue')  $(find vueapp -type f -name '*.js') && /bin/bash translate.sh && gettext-compile --output vueapp/i18n/translations.json locale/en/LC_MESSAGES/opencast.po",
    "zip": "mkdir -p build && zip -r build/Opencast-V$npm_package_version.zip app assets cronjobs locale migrations lib vendor static static_cw LICENSE INSTALL.md README.md bootstrap.php OpenCast.class.php plugin.manifest",
    "docs": "npx redoc-cli build docs/studip_opencast_routes.json --output docs/studip_opencast_routes.html",
    "tests": "php vendor/bin/codecept run --steps"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@popperjs/core": "^2.11.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.47",
    "@vueform/slider": "^2.1.2",
    "axios": "^1.6.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.33.0",
    "css-loader": "^6.8.1",
    "date-fns": "^2.30.0",
    "easygettext": "^2.17.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "focus-trap-vue": "^3.4.0",
    "html-webpack-plugin": "^5.5.3",
    "install": "^0.12.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^6.2.1",
    "redoc-cli": "^0.13.21",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.69.3",
    "sass-loader": "^12.6.0",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.4",
    "vue-axios": "^3.5.2",
    "vue-loader": "^17.3.0",
    "vue-resizable": "^2.1.7",
    "vue-router": "^4.2.5",
    "vue3-gettext": "^2.4.0",
    "vue3-select": "^0.1.2",
    "vuedraggable": "^4.1.0",
    "vuex": "^4.1.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0"
  },
  "browserslist": "> 0.25%, not dead, not IE 11",
  "bugs": {
    "url": "https://github.com/elan-ev/studip-opencast-plugin/issues"
  },
  "homepage": "https://github.com/elan-ev/studip-opencast-plugin#readme",
  "license": "AGPL-3.0-or-later",
