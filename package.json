{
  "name": "studip-opencast-plugin",
  "version": "3.0.0",
  "description": "Stud.IP plugin for Opencast",
  "author": "Till Gl√∂ggler <tgloeggl@uos.de>",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode=development --watch",
    "prebuild": "npm install",
    "prezip": "npm run build && composer install --no-dev",
    "translate": "gettext-extract --attribute v-i18n --attribute v-translate --output locale/en/LC_MESSAGES/opencast_js.pot $(find vueapp -type f -name '*.vue') && /bin/bash translate.sh && gettext-compile --output vueapp/i18n/translations.json locale/en/LC_MESSAGES/opencast.po",
    "zip": "zip -r Opencast-V$npm_package_version.zip app assets classes cronjobs locale migrations lib vendor static LICENSE INSTALL.md README.md bootstrap.php constants.php OpenCast.class.php plugin.manifest"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@vue/cli-service": "^4.5.13",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "core-js": "^3.6.5",
    "css-loader": "^6.2.0",
    "date-fns": "^2.0.0-beta.2",
    "easygettext": "^2.17.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "graphql-tag": "^2.9.0",
    "html-webpack-plugin": "^5.3.2",
    "install": "^0.12.2",
    "npm": "^6.9.2",
    "popper.js": "^1.15.0",
    "portal-vue": "^2.1.7",
    "regenerator-runtime": "^0.13.5",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-axios": "^3.2.5",
    "vue-cli-plugin-apollo": "~0.22.2",
    "vue-gettext": "^2.1.12",
    "vue-loader": "^15.9.8",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1.1",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.8.0"
